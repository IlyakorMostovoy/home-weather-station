.app-bar {
  background-color: #00bcd4;
  box-sizing: border-box;
  width: 100%;
  padding-left: 25px;
  padding-right: 25px;

  &__title {
    padding: 0;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    margin: 0px;
    padding-top: 0px;
    letter-spacing: 0px;
    font-size: 24px;
    font-weight: 400;
    color: rgb(255, 255, 255);
    height: 64px;
    line-height: 64px;
  }
}
